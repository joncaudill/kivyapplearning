#:kivy 2.3.1

<GameScreen>:

    FloatLayout:
        pos: 0, 0
        size: root.width, root.height

        Label:
            text: root.label_text
            pos_hint: {'x': .2, 'y': .2}
        Label:
            text: "I have " + str(root.numeric) + " updogs."
            pos_hint: {'x': .1, 'y': .1}